{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productTitle": {
      "type": "uid"
    },
    "Name": {
      "type": "string"
    },
    "descripcion": {
      "type": "text"
    },
    "imagen": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "total_unidades": {
      "type": "integer"
    },
    "precio_unidad": {
      "type": "decimal"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "products"
    }
  }
}
